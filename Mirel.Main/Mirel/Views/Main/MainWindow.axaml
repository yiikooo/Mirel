                    <TextBlock
                        Classes="eq-width"
                        DockPanel.Dock="Left"
                        FontSize="12.5"
                        HorizontalAlignment="Left"
                        Text="{Binding SelectedTab.Content.ShortInfo}"
                        VerticalAlignment="Center" />
                    <TextBlock
                        Classes="eq-width"
                        DockPanel.Dock="Right"
                        FontSize="12.5"
                        HorizontalAlignment="Right"
                        Margin="0,0,3,0"
                        Text="{Binding Time}"
                        VerticalAlignment="Center" />
