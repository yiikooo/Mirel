        <converter:NavScrollOpacityMaskConverter x:Key="NavScrollOpacityMaskConverter" />
        <converter:NotificationTypeToClassesConverter x:Key="NotificationTypeToClassesConverter" />
    </u:UrsaWindow.Resources>
    <u:UrsaWindow.Styles>
        <Style Selector="FlyoutPresenter.nt /template/ ContentPresenter">
            <Setter Property="Margin" Value="0" />
        </Style>
        <Style Selector="Border#NotificationCard.closing">
            <Setter Property="Opacity" Value="0" />
        </Style>
